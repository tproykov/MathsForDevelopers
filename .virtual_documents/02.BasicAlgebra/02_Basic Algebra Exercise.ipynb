def interpolate_polynomial(points, degree, min_x, max_x):

    NUM_POINTS = 1000
    
    x, y = points[:, 0], points[:, 1]
    poly = Polynomial.fit(x, y, deg = degree)
    
    plot_x = np.linspace(min_x, max_x, NUM_POINTS)
    plot_y = poly(plot_x)
    return plot_x, plot_y

